{
  "presets": [
    "es2015",
    "stage-1",
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              },
              {
                "transform": "react-transform-catch-errors",
                "imports": [
                  "react",
                  "redbox-react"
                ]
              }
            ]
          }
        ],
        "typecheck",
        "transform-runtime"
      ]
    }
  }
}
